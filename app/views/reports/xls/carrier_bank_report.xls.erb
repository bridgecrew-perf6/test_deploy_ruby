    body = []
    body.push "#{carrier_bank.carrier.name}\t"
    body.push "#{carrier_bank.currency}\t"
    body.push "#{carrier_bank.bank_name}\t"
    body.push "#{carrier_bank.acc_name}\t"
    body.push "#{carrier_bank.acc_no}\t"
  %>
<%= body.join("").html_safe %>
<% end %>