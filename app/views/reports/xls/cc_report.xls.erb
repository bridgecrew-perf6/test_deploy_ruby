<% columns = @report.columns %>
<% base = [] %>
<% 19.times{ |i| base.push i.to_s } %>
<%
  head = []
  head.push ""
  head.push "Close Ref\t"
  head.push "IBL Ref\t"
  head.push "Customer\t"
  head.push "Shipper\t"
  head.push "Shipper Ref\t"
  head.push "MBL\t"
  head.push "Invoice No\t"
  head.push "Qty\t"
  head.push "POL\t"
  head.push "Destination\t"
  head.push "USD\t"
  head.push "IDR\t"
  head.push "Invoice Date\t"
  head.push "ETD Date\t"
  head.push "Due Date\t"
  head.push "Payment Date\t"
  head.push "Status\t"
  head.push "Note\t"
%>
<% (base-columns).reverse_each{ |column| head.delete_at(column.to_i) } %>
<%= head.join("").html_safe %>
<% @results.each do |reference| %>
  <% body = report_list_cc(reference, "xls") %>
  <% (base-columns).reverse_each{ |column| body.delete_at(column.to_i) } unless body.blank? %>
<%= body.join("").html_safe unless body.blank? %>
<% end %>